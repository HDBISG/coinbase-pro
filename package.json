{
  "name": "coinbase-pro-node-api",
  "version": "1.0.0",
  "description": "Node.js library for Coinbase Pro",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier -c **/*.{js,md,ts,json}",
    "prettier-write": "npm run prettier -- --write",
    "lint": "eslint --ext .ts,.js ./",
    "lint-fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vansergen/coinbase-pro.git"
  },
  "keywords": [
    "Coinbase",
    "crypto",
    "currency",
    "exchange",
    "API"
  ],
  "author": "Sergey Bakulin",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/vansergen/coinbase-pro/issues"
  },
  "homepage": "https://github.com/vansergen/coinbase-pro#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "rpc-request": "^3.1.1"
  }
}
